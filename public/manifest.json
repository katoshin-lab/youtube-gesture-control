{
  "name": "Youtube Gesture Control",
  "version": "0.1.0",
  "manifest_version": 3,
  "action": {
    "default_title": "Youtube Gesture Control",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "history",
    "scripting"
  ],
  "host_permissions": [
    "http://www.youtube.com/*",
    "https://www.youtube.com/*"
  ]
}